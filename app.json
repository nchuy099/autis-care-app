{
  "expo": {
    "name": "my-expo-app",
    "slug": "my-expo-app",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/GameAssets/GameComponent/puzzle_game_logo.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [],
    "orientation": "default",
    "icon": "./assets/GameAssets/GameComponent/puzzle_game_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/GameAssets/GameComponent/puzzle_game_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/GameAssets/GameComponent/puzzle_game_logo.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}